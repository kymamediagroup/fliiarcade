<!-- bezel.lay -->
<mamelayout version="2"><element name="cabinet"><image file="../../cabinets/destroyr.png"/></element><element name="marquee"><image file="../../marquees/destroyr.png"/></element><element name="flyer"><image file="../../flyers/destroyr.png"/></element>
<element name="bezel">
<image file="bezel.png"/>
</element>
<element name="backdrop">
<image file="destroyr.png"/>
</element>
<element name="backdrop2">
<image file="destroyer_background.png"/>
</element>
<element name="foreground">
<image file="destroyer_foreground.png"/>
</element>
<element name="tint">
<image file="tintover.png"/>
</element>
<view name="Bezel Artwork">
<bounds left="0" top="0" right="16" bottom="9"/>
